{
  "name": "boyanyordanov/php-openapi-test-tools",
  "description": "A library to help test your API based on your OpenAPI Specification",
  "require": {
    "php": "^7.2",
    "psx/api": "^2.5",
    "leko/json-schema-faker": "^1.0",
    "swaggest/json-schema": "^0.12.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3",
    "mockery/mockery": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "OpenAPITestTools\\": "src/",
      "OpenAPITestTools\\Tests\\": "tests/"
    }
  }
}
